Xsession: $(srcdir)/Xsession.in
	sed	-e 's,[@]XSESSION_SHELL[@],$(XSESSION_SHELL),g' \
        -e 's,[@]libexecdir[@],$(libexecdir),g' \
		-e 's,[@]DATADIR[@],$(DATADIR),g' \
		<$(srcdir)/Xsession.in >Xsession

